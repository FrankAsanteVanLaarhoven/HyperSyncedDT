# Application Settings
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0
STREAMLIT_THEME_BASE="light"

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/hypersyncdt
DATABASE_TEST_URL=postgresql://user:password@localhost:5432/hypersyncdt_test

# API Configuration
BACKEND_API_URL=http://localhost:8000
API_KEY=your_api_key_here

# ML Model Settings
MODEL_PATH=/app/models
TORCH_DEVICE=cpu
QUANTUM_MESH_DATA_PATH=/app/data/quantum_mesh_data.h5

# Monitoring and Logging
LOG_LEVEL=INFO
ENABLE_TELEMETRY=false

# Security
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here
CORS_ORIGINS=http://localhost:8501,http://localhost:3000

# Integration Settings
ENABLE_QUANTUM_SIMULATION=true
ENABLE_DIGITAL_TWIN=true
ENABLE_RAG_ASSISTANT=true

# External Services (if needed)
S3_BUCKET=your-bucket-name
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2 